urxvt*termName: rxvt-256color

!*foreground:     rgb:22/22/22
!*background:     rgb:fa/fa/fa
URxvt.background: black
URxvt.foreground: grey
URxvt.saveLines: 10000
!URxvt.tintColor: white
URxvt.scrollBar: false
URxvt.urgentOnBell: true

urxvt*fading: 10

!URxvt*font: xft:Source Code Pro:regular:pixelsize=12.5
!URxvt*font: xft:Bitstream Vera Sans Mono:regular:pixelsize=12:antialias=true
!Xft.dpi: 220
!Xft.antialias: 1
!Xft.hinting:    1
!Xft.hintstyle:  hintfull
!Xft.rgba:       rgb
!xterm*faceName: DejaVu Sans LGC
!xterm*faceSize: 10
!URxvt*font: xft:DejaVu Sans LGC-10
!URxvt.font: xft:DejaVu Sans Mono:pixelsize=12


Xft: antialias: 1
Xft.hinting:    1
Xft.hintstyle:  hintfull

URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2: (\\/[-_\\w\\d]+)+\\.html

